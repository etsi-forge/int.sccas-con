				f_registrationAS(v_cSeq_s);
			    f_selfOrClientSyncAndVerdict(c_prDone, f_getVerdict()); // sync
			
				f_IncCSeq(v_cSeq_s);
				f_setHeaders_INVITE_AS(v_cSeq_s, f_initSipUrl(c_userProfile_PCSCFwithHomeUE), f_initSipUrl(c_userProfile_SCSCFwithHomeUE));
