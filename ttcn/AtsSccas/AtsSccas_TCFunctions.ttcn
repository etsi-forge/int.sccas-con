											));
				tc_ack.start;
				alt {
					[] SIPP.receive(mw_Response_1xx_Base(vc_callId, vc_cSeq)) {
						repeat
					}
					[] SIPP.receive(mw_INVITE_Request_AS(?, ?, ?, ?, *, *)) {
						tc_ack.stop;
						log("*** " & __SCOPE__ & ": PASS: INVITE message was received ***");
