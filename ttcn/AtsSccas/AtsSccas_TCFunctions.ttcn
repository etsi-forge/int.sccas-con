						f_sendResponse(
						m_Response_18XonINVITE_AS_Featurecaps(
											c_statusLine183,
											vc_callId, 
											p_cSeq_s, 
											vc_from, 
											vc_to, 
											vc_via_REG,
											-,
						                    -
